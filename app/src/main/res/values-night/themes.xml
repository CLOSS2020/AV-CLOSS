    <style name="MyDatePickerStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">#555555</item>
        <!--selected Item color-->
        <item name="colorControlActivated">@color/primaryLightColor</item>
        <!-- arrow bordercolor(<>)-->
        <item name="android:selectableItemBackgroundBorderless">@color/blackColor3</item>
        <!-- Highlight item color-->
        <item name="colorControlHighlight">#004DE5</item>
        <!--Calender Background color -->
        <item name="android:windowBackground">#424242</item>
        <!-- Ok Cancel Color-->
        <item name="android:textColor">@color/whiteColor1</item>
        <!-- Week TextColor-->
        <item name="android:textColorSecondary">@color/whiteColor3</item>
        <!-- Calender Number color arrow color (< >) -->
        <item name="android:textColorPrimary">@color/whiteColor1</item>
        <!--day , month-->
        <item name="android:textColorPrimaryInverse">@color/whiteColor1</item>
        <!-- year-->
        <item name="android:textColorSecondaryInverse">@color/whiteColor2</item>

        <item name="android:buttonBarNegativeButtonStyle">
            @style/DatePickerTheme.ButtonBarNegativeButtonStyle
        </item>
        <item name="android:buttonBarPositiveButtonStyle">
            @style/DatePickerTheme.ButtonBarPositiveButtonStyle
